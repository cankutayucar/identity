@{
    ViewData["Title"] = "Users";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@model List<CankutayUcarIdentity.UI.Models.AppUser>

<h2>Üyeler</h2>
<hr />

<table class="table table-bordered table-striped">
    <tr>
        <td>User Id</td>
        <td>User Name</td>
        <td>Email</td>
    </tr>
    @if (Model.Count == 0)
    {
        <tr>
            <td colspan="3">Hiçbir üye yoktur</td>
        </tr>
    }
    else
    {
        foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
            </tr>
        }
    }
</table>

